# Added by OrbStack: 'orb' SSH host for Linux machines
# This only works if it's at the top of ssh_config (before any Host blocks).
# This won't be added again if you remove it.
Include ~/.orbstack/ssh/config

Host LavishPrince
    HostName github.com
    IdentityFile ~/.ssh/LavishPrince
    User git
    IdentitiesOnly yes
Host LavishPrince-bit
    HostName bitbucket.org
    IdentityFile ~/.ssh/LavishPrince
    User git
    IdentitiesOnly yes
Host LavishPrince-lab
    HostName gitlab.com
    IdentityFile ~/.ssh/LavishPrince
    User git
    IdentitiesOnly yes
Host Vinaydg
    HostName github.com
    IdentityFile ~/.ssh/Vinaydg
    User git
    IdentitiesOnly yes
